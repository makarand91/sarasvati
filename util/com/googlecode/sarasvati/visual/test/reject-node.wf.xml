<?xml version="1.0"?>
<process-definition name="reject-node"
                   xmlns="http://sarasvati.googlecode.com/ProcessDefinition">

  <node name="1" isStart="true" >
    <arc to="2"/>
  </node>

  <node name="2" >
  	<arc to="3"/>
	<arc to="1" name="reject"/>
  </node>
  
  <node name="3" type="wait" >
  	<arc to="4"/>
  </node>
  
  <node name="4">
  	<arc to="5"/>
	<arc to="3" name="reject"/>
  </node>
  
  <node name="5" type="wait" >
  	<arc to="6"/>
	<arc to="3" name="reject"/>
  </node>
  
  <node name="6">
  	<arc to="3" name="reject"/>
	<arc to="end"/>
  </node>
  
  <node name="end" type="end"/>
  
</process-definition>